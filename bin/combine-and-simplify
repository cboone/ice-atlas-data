#!/usr/bin/env bash

yarn mapshaper \
  -i geojson/ct-ri-ma-ny-on-qc-nb-me.geojson \
  -dissolve \
  -dissolve2 sliver-control=0 \
  -clip bbox="-77.4448803163,42.0041639122,-66.6042989998,47.5416852124" remove-slivers \
  -filter-slivers sliver-control=0 remove-empty \
  -clean \
  -o geojson/veil.geojson
