#!/usr/bin/env bash

set -e

declare -r start_time="$(date -u '+%Y-%m-%d %H:%M:%S')"
echo "[update] Started at $start_time"

declare -r dir="$(pwd)"

for region in "$@"; do
  echo "[update] Updating $region"

  "$dir/bin/pyosmium-up-to-date" -vvvv --size 10000 "$dir/data/sources/$region.osm.pbf"
done
